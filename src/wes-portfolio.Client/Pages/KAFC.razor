@attribute [Route(PageRoute.KAFCRoute)]
@attribute [Route(PageRoute.Home)]

<MudGrid Justify="Justify.SpaceBetween">
    <MudItem xs="12" Class="my-4" Style="text-align:center;">
        <MudImage Src="images/kafc/kafc-logo.png" Alt="Login Screen" Class="rounded-lg" Fluid="fluid" />
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kafc/login.png" Alt="Login Screen" Class="rounded-lg" Fluid="fluid" Elevation="elevation" />
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kafc/sidebar_HomePage.png" Alt="Home Page" Class="rounded-lg" Fluid="fluid" Elevation="elevation" />
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kafc/bflp_application.png" Alt="BFLP Application" Class="rounded-lg" Fluid="fluid" Elevation="elevation" />
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kafc/bflp_loanpage.png" Alt="BFLP Loan Page" Class="rounded-lg" Fluid="fluid" Elevation="elevation" />
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kafc/homepage.png" Alt="Home Page" Class="rounded-lg" Fluid="fluid" Elevation="elevation" />
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kafc/lavlp_loanpage.png" Alt="Vet Loan Page" Class="rounded-lg" Fluid="fluid" Elevation="elevation" />
    </MudItem>
</MudGrid>

@code {
    bool fluid = true;
    int elevation = 2;
}
