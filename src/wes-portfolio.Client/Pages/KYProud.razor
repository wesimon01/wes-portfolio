@attribute [Route(PageRoute.KYProudRoute)]

<MudGrid Justify="Justify.SpaceBetween">   
    <MudItem xs="12" Class="my-4" Style="text-align:center;">
        <MudImage Src="images/kyproud/kyp-color.png" Alt="kyproud logo" Class="rounded-lg" Fluid="fluid" Width="275"/>
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kyproud/loginwithnotice.png" Alt="login screen" Fluid="fluid" Class="rounded-lg" Elevation="elevation"></MudImage>
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kyproud/application.png" Alt="application img 1" Fluid="fluid" Class="rounded-lg" Elevation="elevation"></MudImage>
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kyproud/applicationcap-2.png" Alt="application img 2" Fluid="fluid" Class="rounded-lg" Elevation="elevation"></MudImage>
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kyproud/application-review.png" Alt="application review" Fluid="fluid" Class="rounded-lg" Elevation="elevation"></MudImage>
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kyproud/memberpage-1.png" Alt="member page 1" Fluid="fluid" Class="rounded-lg" Elevation="elevation"></MudImage>
    </MudItem>
    <MudItem lg="6">
        <MudImage Src="images/kyproud/memberpage-2.png" Alt="member page 2" Fluid="fluid" Class="rounded-lg" Elevation="elevation"></MudImage>
    </MudItem>
</MudGrid>

@code {
    bool fluid = true;
    int elevation = 2;
}
